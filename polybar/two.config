type = internal/xwindow
label = %title:0:30:...%
module-margin-left = 2
format-padding = 2
format-background = #00eeeeee

[module/i3]
type = internal/i3
strip-wsnumbers = true
enable-click = false
index-sort = true
format = <label-state>

label-focused =   ï„‘
