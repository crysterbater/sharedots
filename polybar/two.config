[global/wm]
margin-top = 8
margin-bottom = 0
override-redirect = true

[bar/top]
monitor = eDP1
bottom = false
width = 100%
height = 30
offset-x = 0
offset-y = 0

locale = de_DE.UTF-8
background = #3a3c61
foreground = #678bdc

font-0 = lucy tewii:size=12:antialias=false;1
font-1 = FontAwesome:size=12:antialias=false;2
font-2 = MaterialIcons-Regular:size=12:antialias=false;3
font-3 = Misc Stlarch:size=12:antialias=false;4

modules-left = date1 volume battery cpu
modules-center = workspaces
modules-right = rofi network previous next powermenu

tray-position = false
tray-padding = 1
tray-transparent = false
tray-detached = false
tray-background = #3a3c61

overline-size = 2
overline-color = #bc92f8
underline-size = 2
underline-color = #bc92f8

radius = 2

spacing = 1
padding-left = 1
padding-right = 0
module-margin-left = 1
module-margin-right = 0

[module/volume]
type = internal/volume

format-volume = <ramp-volume><label-volume>
module-margin-right = 0

ramp-volume-0 =ï€¦
ramp-volume-1 =ï€§
ramp-volume-2 =ï€¨
ramp-volume-3 =ï€¨

